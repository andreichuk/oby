{
  "name": "oby",
  "repository": "github:vobyjs/oby",
  "description": "A tiny Observable implementation, the brilliant primitive you need to build a powerful reactive system.",
  "version": "12.0.0",
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "exports": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "benchmark": "tsex benchmark",
    "benchmark:watch": "tsex benchmark --watch",
    "clean": "tsex clean",
    "cleanup": "node tasks/cleanup.js",
    "compile": "tsex compile",
    "compile:mangle": "mangler --folder dist --ext js --props 'fnWithIndex,key,insert,node,getters,setters,properties,selecteds,source,listen,listened,listeners,registerListener,unregisterListener,fn,bool,read,wrap,after,cache,count,emit,stale,source,index,value,valueReadonly,write,signal,before,errors,parent,pausable,target,result,update,toggle,frozen,current,dispose,unstale,inactive,effects,cleanups,contexts,roots,readable,writable,nextCount,observers,prevCount,suspenses,suspended,observables,postdispose,statusCount,statusFresh,registerSelf,registerError,registerEffect,statusExecution,registerCleanup,unregisterEffect,registerSuspense,registerObserver,unregisterSuspense,registerObservable,unregisterObserver,registerRoot,unregisterRoot,observablesLeftover'",
    "compile:watch": "tsex compile --watch",
    "creation": "node tasks/creation.js",
    "memory": "node tasks/memory.js",
    "store": "node tasks/store.js",
    "test:native": "fava '**/native/**'",
    "test:native:watch": "fava -w '**/native/**'",
    "test:sinuous": "bash test/sinuous/index.sh",
    "test": "npm run test:sinuous && npm run test:native",
    "updates": "node tasks/updates.js",
    "prepublishOnly": "npm run clean && npm run compile && npm run compile:mangle && npm run test"
  },
  "keywords": [
    "observable",
    "signal",
    "reactive",
    "reactivity",
    "tiny",
    "small",
    "fast"
  ],
  "devDependencies": {
    "benchloop": "^1.3.2",
    "fava": "^0.0.6",
    "ispy": "^0.1.2",
    "js-simple-mangler": "^1.0.0",
    "tape": "^5.5.3",
    "tsex": "^1.1.2",
    "typescript": "^4.6.4"
  }
}
